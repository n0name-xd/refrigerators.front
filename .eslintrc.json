{
  "extends": "next/core-web-vitals",
  "rules": {
    "quotes": ["error", "double"],
    "semi": "error",
    "max-lines": "warn",
    "no-multiple-empty-lines": ["error", {
      "max": 1
      }],
    "no-unused-vars": ["error", {
       "vars": "all", 
       "args": "after-used", 
       "ignoreRestSiblings": false
      }]
  }
}
